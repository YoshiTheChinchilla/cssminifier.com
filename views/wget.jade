extends layout

block content
  h3.muted Wget
  pre.
    # minifying some CSS
    wget --post-data='input=p { color : red; }' --output-document=- http://cssminifier.com/raw

    # minifying a CSS file (e.g. style.css -> style.min.css)
    wget --post-data="input=`cat style.css`" --output-document=style.min.css http://cssminifier.com/raw

  include ./language-footer.jade
