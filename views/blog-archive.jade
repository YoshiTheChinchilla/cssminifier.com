extends blog

block blogcontent

  h2 Archive

  for year in Object.keys(thisArchive).sort().reverse()
    for month in Object.keys(thisArchive[year]).sort().reverse()
      h3 #{thisArchive[year][month][0].meta.moment.format("MMM YYYY")}
      ul
        for l_post in thisArchive[year][month]
          li #{l_post.meta.moment.format("DD MMM YYYY")} - 
            a(href="./#{l_post.name}")= l_post.meta.title
